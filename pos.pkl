V\u0417\u0430\u043b\u0438\u0432\u0430\u044e \u0434\u0438\u0441\u0442\u0438\u043b\u043b\u0438\u0440\u043e\u0432\u0430\u043d\u043d\u0443\u044e \u0432\u043e\u0434\u0443 (5\u043b \u0432 \u0430\u0432\u0442\u043e\u043c\u0430\u0433\u0430\u0437\u0438\u043d\u0430\u0445) \u0441 \u0430\u0440\u043e\u043c\u0430\u0442\u0438\u0437\u0430\u0442\u043e\u0440\u043e\u043c \u043d\u0430 \u0441\u0432\u043e\u0439 \u0432\u043a\u0443\u0441.
p1
aV\u041f\u043e\u043b\u044c\u0437\u0443\u044f\u0441\u044c \u0442\u0430\u043a\u0438\u043c \u0443\u0442\u044e\u0433\u043e\u043c \u0431\u043e\u043b\u044c\u0448\u0435 \u0433\u043e\u0434\u0430. \u041f\u043e\u043a\u0430 \u043d\u0438\u043a\u0430\u043a\u0438\u0445 \u043f\u043e\u043b\u043e\u043c\u043e\u043a \u043d\u0435 \u0431\u044b\u043b\u043e, \u043d\u0430\u0432\u0435\u0440\u043d\u043e\u0435 \u043f\u043e\u043f\u0430\u043b\u0430\u0441\u044c \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u0430\u044f \u043c\u043e\u0434\u0435\u043b\u044c. \u041d\u0440\u0430\u0432\u0438\u0442\u0441\u044f \u043c\u043e\u0449\u043d\u043e\u0441\u0442\u044c \u044d\u0442\u043e\u0433\u043e \u0443\u0442\u044e\u0433\u0430, \u0432\u0435\u0440\u0442\u0438\u043a\u0430\u043b\u044c\u043d\u043e\u0435 \u043e\u0442\u043f\u0430\u0440\u0438\u0432\u0430\u043d\u0438\u0435, \u043f\u0430\u0440\u043e\u0432\u043e\u0439 \u0443\u0434\u0430\u0440 \u0434\u043e\u0441\u0442\u0430\u0442\u043e\u0447\u043d\u043e \u0441\u0438\u043b\u044c\u043d\u044b\u0439. \u041c\u043e\u0434\u0435\u043b\u044c \u043d\u0435 \u0438\u0437 \u043b\u0451\u0433\u043a\u0438\u0445. \u041e\u0442\u0433\u043b\u0430\u0436\u0438\u0432\u0430\u0435\u0442 \u0445\u043e\u0440\u043e\u0448\u043e. \u041f\u0435\u0440\u0438\u043e\u0434\u0438\u0447\u0435\u0441\u043a\u0438 \u043e\u0447\u0438\u0449\u0430\u044e , \u0438 \u0442\u043e\u0433\u0434\u0430 \u043d\u0438\u043a\u0430\u043a\u043e\u0439 \u043d\u0430\u043a\u0438\u043f\u0438 \u043d\u0435\u0442 \u0438 \u0441\u043b\u0435\u0434\u043e\u0432 \u0442\u043e\u0436\u0435.
p2
aV\u041c\u0430\u043c\u0430 \u043d\u0435 \u043d\u0430\u0440\u0430\u0434\u0443\u0435\u0442\u0441\u044f \u043d\u0430 \u043d\u0435\u0433\u043e, \u043e\u0442\u043b\u0438\u0447\u043d\u043e \u0433\u043b\u0430\u0434\u0438\u0442, \u0447\u0438\u0441\u0442\u0438\u0442\u0441\u044f \u0442\u043e\u0436\u0435 \u0431\u0435\u0437 \u043f\u0440\u043e\u0431\u043b\u0435\u043c, \u0446\u0435\u043d\u0430 \u043a\u043e\u043d\u0435\u0447\u043d\u043e \u043d\u0430 \u0444\u043e\u043d\u0435 \u0434\u0440\u0443\u0433\u0438\u0445 \u043e\u0442\u043b\u0438\u0447\u0430\u0435\u0442\u0441\u044f, \u043d\u043e \u0437\u0430 \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u043e \u043d\u0443\u0436\u043d\u043e \u043f\u043b\u0430\u0442\u0438\u0442\u044c. \u0415\u0441\u043b\u0438 \u0443\u0442\u044e\u0433, \u0442\u043e \u0442\u043e\u043b\u044c\u043a\u043e \u0424\u0438\u043b\u0438\u043f\u0441.
p3
aV\u041f\u043e\u043b\u044c\u0437\u0443\u044e\u0441\u044c \u0431\u043e\u043b\u0435\u0435 3-\u0445 \u043b\u0435\u0442, \u043e\u0447\u0435\u043d\u044c \u0434\u043e\u0432\u043e\u043b\u0435\u043d \u0434\u0430\u043d\u043d\u044b\u043c \u0443\u0442\u044e\u0433\u043e\u043c
p4
aV\u041e\u0447\u0435\u043d\u044c \u0434\u043e\u043b\u0433\u043e \u0432\u044b\u0431\u0438\u0440\u0430\u043b \u0443\u0442\u044e\u0433! \u041c\u043e\u0449\u043d\u043e\u0441\u0442\u044c, \u043f\u043e\u0434\u043e\u0448\u0432\u0430, \u0443\u0434\u043e\u0431\u0441\u0442\u0432\u043e \u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u043d\u0438\u044f... \u041e\u0441\u0442\u0430\u043d\u043e\u0432\u0438\u043b \u0441\u0432\u043e\u0439 \u0432\u044b\u0431\u043e\u0440 \u043d\u0430 4870 \u0438 \u043d\u0435 \u043f\u043e\u0436\u0430\u043b\u0435\u043b! \u042f \u043f\u043e\u043a\u0443\u043f\u0430\u043b \u0435\u0433\u043e \u0435\u0449\u0451 \u0432 2012 \u0433\u043e\u0434\u0443! \u0438 \u0441 \u0442\u043e\u0433\u043e \u043c\u043e\u043c\u0435\u043d\u0442\u0430 \u043d\u0438 \u0440\u0430\u0437\u0443 \u043d\u0435 \u043f\u0440\u0438\u0436\u0451\u0433 \u043d\u0438 \u0431\u0440\u044e\u043a\u0438, \u043d\u0438 \u0440\u0443\u0431\u0430\u0448\u043a\u0443! \u041e\u0447\u0435\u043d\u044c \u0443\u0434\u043e\u0431\u043d\u043e \u0433\u043b\u0430\u0434\u0438\u0442\u044c \u0432\u043e\u0437\u043b\u0435 \u043f\u0443\u0433\u043e\u0432\u0438\u0446 \u0438 \u0432\u043e\u0440\u043e\u0442\u043d\u0438\u043a \u0441 \u043a\u0430\u0440\u043c\u0430\u043d\u0430\u043c\u0438. \u0422\u0435\u043f\u0435\u0440\u044c \u044f \u043f\u043e\u043d\u0438\u043c\u0430\u044e \u0447\u0442\u043e \u0442\u0430\u043a\u043e\u0435 \u043f\u0430\u0440\u043e\u0432\u043e\u0439 \u0423\u0414\u0410\u0420! \u041f\u0440\u043e\u0433\u043b\u0430\u0436\u0438\u0432\u0430\u0435\u0442 \u0438\u0434\u0435\u0430\u043b\u044c\u043d\u043e \u0432\u0441\u0451! \u042f \u0434\u043e\u0432\u043e\u043b\u0435\u043d!
p5
aV\u041c\u043e\u0434\u0435\u043b\u044c \u043e\u0447\u0435\u043d\u044c \u0445\u043e\u0440\u043e\u0448\u0430\u044f, \u0434\u0435\u0439\u0441\u0442\u0432\u0438\u0442\u0435\u043b\u044c\u043d\u043e \u043e\u0442\u0443\u0442\u044e\u0436\u0438\u0432\u0430\u0435\u0442 \u0445\u043e\u0440\u043e\u0448\u043e, \u043c\u043e\u0449\u043d\u044b\u0439 \u043f\u0430\u0440\u043e\u0432\u043e\u0439 \u0443\u0434\u0430\u0440, \u0438\u043d\u043e\u0433\u0434\u0430 \u043f\u0438\u0448\u0443\u0442, \u0447\u0442\u043e \u0441 \u0444\u0443\u043d\u043a\u0446\u0438\u0435\u0439 \u0438\u043e\u043d\u0438\u0437\u0430\u0446\u0438\u0438 \u043f\u0430\u0440\u0430, \u0447\u0442\u043e \u0431\u0435\u0437 \u043d\u0435\u0435 - \u0440\u0430\u0437\u043d\u0438\u0446\u044b \u043d\u0435\u0442, \u0433\u043e\u0442\u043e\u0432\u0430 \u043f\u043e\u0441\u043f\u043e\u0440\u0438\u0442\u044c, \u0447\u0442\u043e \u0432\u0441\u0435-\u0442\u0430\u043a\u0438 \u0440\u0430\u0437\u043d\u0438\u0446\u0430 \u0435\u0441\u0442\u044c: \u043f\u0440\u0438 \u0432\u043a\u043b\u044e\u0447\u0435\u043d\u0438\u0438 \u044d\u0442\u043e\u0439 \u043a\u043d\u043e\u043f\u043a\u0438 \u043f\u0430\u0440 \u043f\u043e\u0441\u0442\u0443\u043f\u0430\u0435\u0442 \u0441\u0443\u0445\u043e\u0439, \u043e\u043d \u0440\u0430\u0437\u0431\u0438\u0432\u0430\u0435\u0442\u0441\u044f \u043d\u0430 \u0438\u043e\u043d\u044b - \u043c\u0435\u043b\u044c\u0447\u0430\u0439\u0448\u0438\u0435 \u0447\u0430\u0441\u0442\u0438\u0446\u044b, \u0441\u043b\u0435\u0434\u043e\u0432\u0430\u0442\u0435\u043b\u044c\u043d\u043e \u0438 \u043f\u0430\u0440 \u0432\u044b\u0445\u043e\u0434\u0438\u0442 \u0441\u0443\u0445\u043e\u0439 \u0438 \u0433\u043b\u0443\u0431\u0436\u0435 \u043f\u0440\u043e\u043d\u0438\u043a\u0430\u0435\u0442. \u0411\u0435\u043b\u044c\u0435 \u043d\u0435 \u043d\u0430\u0434\u043e \u043e\u0442\u0441\u0442\u0430\u0438\u0432\u0430\u0442\u044c \u0443\u0442\u044e\u0433\u043e\u043c, \u0447\u0442\u043e\u0431\u044b \u0432\u044b\u0441\u0443\u0448\u0438\u0442\u044c,  \u0432\u0435\u0434\u044c, \u043a\u0430\u043a \u0438\u0437\u0432\u0435\u0441\u0442\u043d\u043e, \u043d\u0435 \u0434\u043e\u0441\u0443\u0448\u0435\u043d\u043d\u043e\u0435 \u0431\u0435\u043b\u044c\u0435 \u0431\u044b\u0441\u0442\u0440\u043e \u043c\u043d\u0435\u0442\u0441\u044f.  \u0424\u0443\u043d\u043a\u0446\u0438\u0435\u0439  \u0438\u043e\u043d\u0438\u0437\u0430\u0446\u0438\u044f \u043d\u0430\u0434\u0435\u043b\u0435\u043d\u044b \u0441\u043e\u0432\u0441\u0435\u043c \u043d\u0435 \u043c\u043d\u043e\u0433\u043e \u043c\u043e\u0434\u0435\u043b\u0435\u0439. \u0422\u044f\u0436\u0435\u043b\u043e\u0432\u0430\u0442 \u0434\u043b\u044f \u043c\u0435\u043d\u044f, \u043d\u043e \u0434\u043e \u044d\u0442\u043e\u0433\u043e \u0443 \u043c\u0435\u043d\u044f \u0431\u044b\u043b \u043d\u0435 \u0443\u0442\u044e\u0433, \u0430 "\u043c\u044b\u043b\u044c\u043d\u0438\u0446\u0430" 20 \u043b\u0435\u0442\u043d\u0435\u0439 \u0434\u0430\u0432\u043d\u043e\u0441\u0442\u0438. \u0425\u043e\u0447\u0443 \u0441\u043a\u0430\u0437\u0430\u0442\u044c, \u0447\u0442\u043e \u0443\u0442\u044e\u0433  \u0445\u043e\u0440\u043e\u0448\u043e \u0435\u0437\u0434\u0438\u0442 \u043f\u043e \u043f\u043e\u0432\u0435\u0440\u0445\u043d\u043e\u0441\u0442\u0438 \u0438 \u0441\u043a\u043e\u043b\u044c\u0437\u0438\u0442. \u041f\u043e\u0434\u043e\u0448\u0432\u0430 \u0445\u043e\u0440\u043e\u0448\u0430\u044f, \u043d\u0430\u0434\u0435\u0436\u043d\u0430\u044f. \u0424\u0443\u043d\u043a\u0446\u0438\u044f \u0430\u0432\u0442\u043e\u043e\u0442\u043a\u043b\u044e\u0447\u0435\u043d\u0438\u044f \u0432\u043e\u043e\u0431\u0449\u0435 \u0431\u0435\u0437 \u0441\u043b\u043e\u0432 \u043e\u0446\u0435\u043d\u0438\u043c\u0430. \u041d\u0435 \u0442\u0435\u0447\u0435\u0442, \u043d\u0438\u0447\u0435\u0433\u043e \u043d\u0438\u0433\u0434\u0435 \u043d\u0435 \u0445\u0440\u0443\u0441\u0442\u0438\u0442 \u0432 \u043a\u043e\u0440\u043f\u0443\u0441\u0435, \u043a\u0430\u043a \u043a\u0442\u043e-\u0442\u043e \u043f\u0438\u0448\u0435\u0442. \u041d\u0430\u0434\u0435\u0436\u0435\u043d, \u0445\u043e\u0440\u043e\u0448\u0430\u044f \u0434\u043b\u0438\u043d\u0430 \u0448\u043d\u0443\u0440\u0430. \u0423\u0434\u043e\u0431\u043d\u044b\u0439 \u043d\u043e\u0441\u0438\u043a
p6
aV\u0421 \u043f\u043e\u044f\u0432\u043b\u0435\u043d\u0438\u0435\u043c \u0432 \u0434\u043e\u043c\u0435 \u0434\u0435\u0442\u0435\u0439 \u043e\u0431\u044a\u0435\u043c\u044b \u0441\u0442\u0438\u0440\u043a\u0438-\u0433\u043b\u0430\u0436\u043a\u0438 \u0441\u0443\u0449\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u043e \u0443\u0432\u0435\u043b\u0438\u0447\u0438\u043b\u0438\u0441\u044c, \u0433\u043b\u0430\u0434\u0438\u0442\u044c \u0431\u0435\u043b\u044c\u0435 \u043c\u043d\u0435 \u043f\u0440\u0438\u0445\u043e\u0434\u0438\u0442\u0441\u044f \u043f\u0440\u0430\u043a\u0442\u0438\u0447\u0435\u0441\u043a\u0438 \u043a\u0430\u0436\u0434\u044b\u0439 \u0434\u0435\u043d\u044c. \u042f \u043e\u0442\u043d\u043e\u0448\u0443\u0441\u044c \u043a \u0442\u043e\u0439 \u043a\u0430\u0442\u0435\u0433\u043e\u0440\u0438\u0438 \u043b\u044e\u0434\u0435\u0439, \u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u0433\u043b\u0430\u0434\u044f\u0442 \u0431\u0435\u043b\u044c\u0435 "\u043e\u0442 \u0438 \u0434\u043e" - \u043d\u0430\u0447\u0438\u043d\u0430\u044f \u043f\u043e\u0441\u0442\u0435\u043b\u044c\u043d\u044b\u043c \u0431\u0435\u043b\u044c\u0435\u043c \u0438 \u0437\u0430\u043a\u0430\u043d\u0447\u0438\u0432\u0430\u044f \u043d\u043e\u0441\u043a\u0430\u043c\u0438 \u0441 \u043a\u043e\u043b\u0433\u043e\u0442\u043a\u0430\u043c\u0438:) \u0412\u043e\u0442 \u043a\u0430\u043a-\u0442\u043e \u0441 \u0434\u0435\u0442\u0441\u0442\u0432\u0430 \u043c\u0430\u043c\u0430 \u043c\u0435\u043d\u044f \u043f\u0440\u0438\u0443\u0447\u0438\u043b\u0430. \u0418 \u0434\u0435\u043b\u043e \u0434\u0430\u0436\u0435 \u043d\u0435 \u0432 \u0433\u0438\u0433\u0438\u0435\u043d\u0435, \u044f \u0434\u0435\u043b\u0430\u044e \u044d\u0442\u043e \u0447\u0438\u0441\u0442\u043e \u0438\u0437 \u044d\u0441\u0442\u0435\u0442\u0438\u0447\u0435\u0441\u043a\u0438\u0445 \u0441\u043e\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u0439 - \u043d\u0435 \u043b\u044e\u0431\u043b\u044e \u043a\u043e\u0433\u0434\u0430 \u043e\u0434\u0435\u0436\u0434\u0430 \u0432\u044b\u0433\u043b\u044f\u0434\u0438\u0442 \u043d\u0435\u043e\u043f\u0440\u044f\u0442\u043d\u043e, \u0434\u0430 \u0438 \u0441\u043f\u0430\u0442\u044c \u043d\u0430 \u0433\u043b\u0430\u0436\u0435\u043d\u043e\u043c \u043f\u043e\u0441\u0442\u0435\u043b\u044c\u043d\u043e\u043c \u0431\u0435\u043b\u044c\u0435 \u043a\u0443\u0434\u0430 \u043f\u0440\u0438\u044f\u0442\u043d\u0435\u0435:) \u041f\u0440\u043e\u0447\u0438\u0442\u0430\u043b\u0430 \u0432 \u0438\u043d\u0442\u0435\u0440\u043d\u0435\u0442\u0435 \u043e\u0437\u044b\u0432\u044b \u043e\u0431 \u044d\u0442\u043e\u043c \u0443\u0442\u044e\u0433\u0435, \u0438 \u0440\u0435\u0448\u0438\u043b\u0430 \u043f\u043e\u043f\u0440\u043e\u0431\u043e\u0432\u0430\u0442\u044c. \u0421 \u044d\u0442\u043e\u0433\u043e \u043c\u043e\u043c\u0435\u043d\u0442\u0430 ,\u0433\u043b\u0430\u0436\u043a\u0430 \u0437\u0430\u043d\u0438\u043c\u0430\u0435\u0442 \u043c\u0438\u043d\u0438\u043c\u0443\u043c \u0432\u0440\u0435\u043c\u0435\u043d\u0438.\u0420\u0435\u043a\u043e\u043c\u0435\u043d\u0434\u0443\u044e.
p7
aV\u0414\u0430\u0432\u043d\u043e \u0438\u0441\u043a\u0430\u043b\u0430 \u043d\u0435\u0447\u0442\u043e \u043f\u043e\u0434\u043e\u0431\u043d\u043e\u0435 \u0447\u0442\u043e\u0431\u044b \u0431\u044b\u043b \u043c\u043e\u0449\u043d\u044b\u0439 \u0438 \u0442\u044f\u0436\u0435\u043b\u044b\u0439 \u0432 \u043c\u0435\u0440\u0443 , \u043a\u043e\u0433\u0434\u0430 \u0443\u0432\u0438\u0434\u0435\u043b\u0430 \u0435\u0433\u043e \u043f\u0440\u043e\u0441\u0442\u043e \u0432\u043b\u044e\u0431\u0438\u043b\u0430\u0441\u044c ! \u041e\u0447\u0435\u043d\u044c \u043d\u0440\u0430\u0432\u0438\u0442\u0441\u044f , \u043f\u043e\u043b\u043d\u043e\u0441\u0442\u044c\u044e \u043e\u043f\u0440\u0430\u0432\u0434\u0430\u043b \u043e\u0436\u0438\u0434\u0430\u043d\u0438\u044f !!!
p8
aV\u041e\u0441\u0442\u0430\u043b\u0430\u0441\u044c \u0434\u043e\u0432\u043e\u043b\u044c\u043d\u0430, \u0433\u043e\u0440\u0430\u0437\u0434\u043e \u043b\u0443\u0447\u0448\u0435 \u0447\u0435\u043c \u043f\u0440\u0435\u0434\u044b\u0434\u0443\u0449\u0438\u0439 \u0443\u0442\u044e\u0433 \u0444\u0438\u0440\u043c\u044b Bosch. \u0434\u0430\u0436\u0435 \u0441\u0430\u043c\u044b\u0435 \u043f\u0435\u0440\u0435\u0441\u0443\u0448\u0435\u043d\u043d\u044b\u0435 \u0432\u0435\u0449\u0438 \u0440\u0430\u0437\u0433\u043b\u0430\u0436\u0438\u0432\u0430\u0435\u0442 \u0441 \u043f\u0435\u0440\u0432\u043e\u0433\u043e \u0440\u0430\u0437\u0430., \u0441\u043a\u043e\u043b\u044c\u0437\u0438\u0442 \u0445\u043e\u0440\u043e\u0448\u043e + \u0444\u0443\u043d\u043a\u0446\u0438\u044f \u0430\u0432\u0442\u043e\u043e\u0442\u043a\u043b\u044e\u0447\u0435\u043d\u0438\u044f \u043f\u043e\u0440\u0430\u0434\u043e\u0432\u0430\u043b\u0430!))) \u000a\u043e\u0446\u0435\u043d\u043a\u0430 \u0438\u0437 5 \u0432\u0441\u0435 5!!! \u0440\u0435\u043a\u043e\u043c\u0435\u043d\u0434\u0443\u044e!
p9
aV\u041c\u043d\u0435 \u0438 \u043c\u043e\u0435 \u0436\u0435\u043d\u0435 \u0443\u0442\u044e\u0433 \u043f\u043e\u043d\u0440\u0430\u0432\u0438\u043b\u0441\u044f.
p10
aV\u041f\u0440\u0435\u043a\u0440\u0430\u0441\u043d\u0430\u044f \u043c\u043e\u0434\u0435\u043b\u044c. \u0421\u0442\u043e\u0438\u0442 \u0442\u043e\u0433\u043e, \u0438 \u0442\u0435\u0445 \u0434\u0435\u043d\u0435\u0433. \u041d\u0435 \u043e\u0436\u0438\u0434\u0430\u043b, \u0447\u0442\u043e \u0442\u0430\u043a \u043b\u0435\u0433\u043a\u043e \u0433\u043b\u0430\u0434\u0438\u0442\u044c. \u0421\u0430\u043c \u0431\u044b\u043b \u043f\u0440\u0438\u044f\u0442\u043d\u043e \u0443\u0434\u0438\u0432\u043b\u0435\u043d, \u0430 \u0436\u0435\u043d\u0430 \u0432\u043e\u043e\u0431\u0449\u0435 \u0432 \u0432\u043e\u0441\u0442\u043e\u0440\u0433\u0435 \u043e\u0441\u0442\u0430\u043b\u0430\u0441\u044c.\u000a\u0421\u043e\u0432\u0435\u0442\u0443\u044e, \u043f\u043e\u0442\u043e\u043c\u0443 \u043a\u0430\u043a \u0432\u044b\u0431\u0438\u0440\u0430\u043b \u043f\u0430\u0440\u043e\u0432\u0443\u044e \u0441\u0442\u0430\u043d\u0446\u0438\u044e, \u043d\u043e \u0442\u0435\u043f\u0435\u0440\u044c \u043e\u043a\u043e\u043d\u0447\u0430\u0442\u0435\u043b\u044c\u043d\u043e \u0443\u0431\u0435\u0434\u0438\u043b\u0441\u044f \u0432 \u0442\u043e\u043c, \u0447\u0442\u043e \u0435\u0441\u043b\u0438 \u0431\u0435\u043b\u044c\u044f \u043d\u0435 \u043e\u0433\u0440\u043e\u043c\u043d\u0430\u044f \u043a\u0443\u0447\u0430, \u0438 \u0412\u044b \u043d\u0435 \u0442\u0440\u0430\u0442\u0438\u0442\u0435 \u043f\u043e \u0447\u0430\u0441\u0443 \u0432 \u0434\u0435\u043d\u044c \u043d\u0430 \u0433\u043b\u0430\u0436\u043a\u0443, \u0441\u0442\u043e\u0438\u0442 \u043a\u0443\u043f\u0438\u0442\u044c \u0438\u043c\u0435\u043d\u043d\u043e \u0442\u0430\u043a\u043e\u0439 \u0443\u0442\u044e\u0433. \u041f\u043e\u0432\u0435\u0440\u044c\u0442\u0435, \u0412\u0430\u043c \u043f\u043e\u043d\u0440\u0430\u0432\u0438\u0442\u0441\u044f.
p11
aV\u0423\u0442\u044e\u0433 \u043f\u043e\u043a\u0443\u043f\u0430\u043b \u0432 \u0432 \u043e\u0434\u043d\u043e\u043c \u0438\u0437 \u0441\u0435\u0442\u0435\u0432\u044b\u0445 \u043c\u0430\u0433\u0430\u0437\u0438\u043d\u043e\u0432. \u041f\u0435\u0440\u0432\u044b\u0439 \u0443\u0442\u044e\u0433 \u043e\u043a\u0430\u0437\u0430\u043b\u0441\u044f \u0431\u0440\u0430\u043a\u043e\u0432\u0430\u043d\u043d\u044b\u0439, \u0441\u0430\u043c\u043e\u043f\u0440\u043e\u0438\u0437\u0432\u043e\u043b\u044c\u043d\u043e \u0432\u044b\u043a\u043b\u044e\u0447\u0430\u043b\u0441\u044f \u043f\u043e\u0441\u043b\u0435 3 \u043c\u0438\u043d\u0443\u0442 \u0440\u0430\u0431\u043e\u0442\u044b. \u041f\u043e\u0441\u043b\u0435 \u0437\u0430\u043c\u0435\u043d\u044b \u043d\u0430 \u043d\u043e\u0432\u044b\u0439 \u0443\u0442\u044e\u0433, \u0431\u0435\u0434 \u043d\u0435 \u0437\u043d\u0430\u044e. \u041e\u043d \u043e\u0442\u043b\u0438\u0447\u043d\u043e \u0441\u043f\u0440\u0430\u0432\u043b\u044f\u0435\u0442\u0441\u044f \u0441\u043e \u0441\u0432\u043e\u0438\u043c\u0438 \u0437\u0430\u0434\u0430\u0447\u0430\u043c\u0438. \u0414\u043e \u044d\u0442\u043e\u0433\u043e \u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u043b\u0441\u044f Braun, \u0434\u0430\u043a \u0442\u043e\u0442 \u043f\u043e\u0441\u0442\u043e\u044f\u043d\u043d\u043e \u043f\u043e\u0434\u0442\u0435\u043a\u0430\u043b \u0438 \u043f\u043b\u043e\u0445\u043e \u0433\u043b\u0430\u0434\u0438\u043b. \u0421 \u044d\u0442\u0438\u043c \u0430\u043f\u043f\u0430\u0440\u0430\u0442\u043e\u043c \u0442\u0430\u043a\u0438\u0445 \u043f\u0440\u043e\u0431\u043b\u0435\u043c \u043d\u0435\u0442. \u0420\u0435\u043a\u043e\u043c\u0435\u043d\u0434\u0443\u044e.
p12
aV\u0414\u043b\u044f \u043c\u0435\u043d\u044f \u044d\u0442\u043e \u0438\u0434\u0435\u0430\u043b\u044c\u043d\u044b\u0439 \u0443\u0442\u044e\u0433. \u0424\u0443\u043d\u043a\u0446\u0438\u044f \u0430\u0432\u0442\u043e\u043e\u0442\u043a\u043b\u044e\u0447\u0435\u043d\u0438\u044f \u0441\u0442\u0440\u0430\u0445\u0443\u0435\u0442 \u043e\u0442 \u043f\u043e\u0436\u0430\u0440\u0430. \u041f\u0440\u043e\u0432\u0435\u0440\u044f\u043b - \u0440\u0430\u0431\u043e\u0442\u0430\u0435\u0442. \u041f\u043e \u0442\u043a\u0430\u043d\u0438 \u043b\u0435\u0433\u043a\u043e \u0434\u0432\u0438\u0436\u0435\u0442\u0441\u044f \u0437\u0430 \u0441\u0447\u0435\u0442 \u043f\u043e\u0434\u043e\u0448\u0432\u044b. \u041d\u0430\u0433\u0440\u0435\u0432\u0430\u0435\u0442\u0441\u044f \u0431\u044b\u0441\u0442\u0440\u043e.\u000a\u041e\u0447\u0435\u043d\u044c \u043f\u043e\u043d\u0440\u0430\u0432\u0438\u043b\u043e\u0441\u044c, \u0447\u0442\u043e \u0432 \u043d\u0435\u0433\u043e \u043d\u0443\u0436\u043d\u043e \u043d\u0430\u043b\u0438\u0432\u0430\u0442\u044c \u0432\u043e\u0434\u043e\u043f\u0440\u043e\u0432\u043e\u0434\u043d\u0443\u044e \u0432\u043e\u0434\u0443 (\u0434\u0430\u0436\u0435 \u043d\u0430\u043f\u0438\u0441\u0430\u043d\u043e, \u0447\u0442\u043e \u043e\u0447\u0438\u0449\u0435\u043d\u043d\u0443\u044e \u043d\u0435\u043b\u044c\u0437\u044f \u043d\u0430\u043b\u0438\u0432\u0430\u0442\u044c).
p13
aV\u0412\u044b\u0431\u0438\u0440\u0430\u043b\u0441\u044f \u0441\u0435\u0439 \u0443\u0442\u044e\u0433 \u0434\u043e\u0432\u043e\u043b\u044c\u043d\u043e \u0434\u043e\u043b\u0433\u043e. \u0421\u043d\u0430\u0447\u0430\u043b\u0430 \u043f\u043e\u043d\u0440\u0430\u0432\u0438\u043b\u0441\u044f \u0447\u0438\u0441\u0442\u043e \u0432\u043d\u0435\u0448\u043d\u0435, \u043f\u043e\u0442\u043e\u043c \u043f\u043e \u0445\u0430\u0440\u0430\u043a\u0442\u0435\u0440\u0438\u0441\u0442\u0438\u043a\u0430\u043c.\u000a\u0418\u0437 \u043c\u0438\u043d\u0443\u0441\u043e\u0432 \u0431\u044b\u043b\u0430 \u043e\u0434\u043d\u0430 \u043d\u0435\u043f\u043e\u043d\u044f\u0442\u043a\u0430: \u043f\u043b\u0435\u0432\u0430\u043b\u0441\u044f \u043d\u0430\u043a\u0438\u043f\u044c\u044e \u0443\u0436\u0435 \u0432 \u043f\u0435\u0440\u0432\u044b\u0435 \u0434\u043d\u0438 \u044d\u043a\u0441\u043f\u043b\u0443\u0430\u0442\u0430\u0446\u0438\u0438, \u0445\u043e\u0442\u044f \u0437\u0430\u043b\u0438\u0432\u0430\u043b\u0430 \u0432\u043e\u0434\u0443 \u0438\u0437 \u043d\u043e\u0432\u043e\u0433\u043e \u0444\u0438\u043b\u044c\u0442\u0440\u0430. \u0420\u0435\u0448\u0438\u043b\u0430\u0441\u044c \u043f\u0440\u043e\u0431\u043b\u0435\u043c\u0430 \u0441 \u043f\u043e\u043c\u043e\u0449\u044c\u044e \u0434\u0438\u0441\u0442\u0438\u043b\u043b\u0438\u0440\u043e\u0432\u0430\u043d\u043d\u043e\u0439 \u0432\u043e\u0434\u044b. \u0418\u0437\u0440\u0430\u0441\u0445\u043e\u0434\u043e\u0432\u0430\u043b\u0430 \u044f \u0435\u0451 \u043e\u043a\u043e\u043b\u043e 4-\u0445 \u043b\u0438\u0442\u0440\u043e\u0432, \u0430 \u043f\u043e\u0442\u043e\u043c, \u043a\u043e\u0433\u0434\u0430 \u0441\u043d\u043e\u0432\u0430 \u0437\u0430\u043b\u0438\u043b\u0430 \u0438\u0437 \u0444\u0438\u043b\u044c\u0442\u0440\u0430 -  \u043f\u0440\u043e\u0431\u043b\u0435\u043c\u044b \u043d\u0435 \u0441\u0442\u0430\u043b\u043e. \u0421 \u0442\u0435\u0445 \u043f\u043e\u0440 \u0432\u0441\u0451 \u0447\u0438\u0441\u0442\u043e. \u000a\u0418\u0437 \u043f\u043b\u044e\u0441\u0438\u043a\u043e\u0432 - \u0445\u043e\u0442\u044c \u0438\u043d\u043e\u0433\u0434\u0430 \u0438 \u043d\u0435 \u0432\u044b\u043b\u0438\u0432\u0430\u044e \u043e\u0441\u0442\u0430\u0432\u0448\u0443\u044e\u0441\u044f \u0432\u043e\u0434\u0443, \u043d\u0438\u0433\u0434\u0435 \u043d\u0435 \u0440\u0430\u0437\u0443 \u043d\u0435 \u043f\u0440\u043e\u0442\u0435\u043a\u0430\u043b. \u0428\u043d\u0443\u0440 \u0434\u043b\u0438\u043d\u043d\u044b\u0439, \u043f\u043e\u0441\u043b\u0435 \u043f\u0440\u0435\u0434\u044b\u0434\u0443\u0449\u0435\u0433\u043e \u0443\u0442\u044e\u0433\u0430 \u044d\u0442\u043e \u0440\u0430\u0434\u0443\u0435\u0442. \u041c\u043d\u043e\u0433\u043e \u0447\u0438\u0442\u0430\u043b\u0430 \u043e \u0431\u0435\u0441\u043f\u043e\u043b\u0435\u0437\u043d\u043e\u0441\u0442\u0438 \u0438\u043e\u043d\u0438\u0437\u0430\u0446\u0438\u0438.\u0417\u0430\u043c\u0435\u0442\u0438\u043b\u0430, \u0447\u0442\u043e \u043f\u0440\u0438 \u043f\u0440\u043e\u0433\u043b\u0430\u0436\u0438\u0432\u0430\u043d\u0438\u0438 \u0447\u0451\u0440\u043d\u044b\u0445 \u0432\u0435\u0449\u0435\u0439 \u0441 \u043b\u0438\u0446\u0435\u0432\u043e\u0439 \u0441\u0442\u043e\u0440\u043e\u043d\u044b \u0431\u0435\u0437 \u0441\u043f\u0435\u0446\u0438\u0430\u043b\u044c\u043d\u043e\u0439 \u0441\u0435\u0442\u043a\u0438 \u0431\u043b\u0435\u0441\u0442\u044f\u0442 \u043c\u0435\u043d\u044c\u0448\u0435, \u0447\u0435\u043c \u0441 \u0432\u044b\u043a\u043b\u044e\u0447\u0435\u043d\u043d\u043e\u0439 \u0444\u0443\u043d\u043a\u0446\u0438\u0435\u0439. \u000a\u0421\u0435\u0439\u0447\u0430\u0441 \u0432\u044b\u0431\u0438\u0440\u0430\u044e \u0443\u0442\u044e\u0433 \u0432 \u043f\u043e\u0434\u0430\u0440\u043e\u043a \u043c\u0430\u043c\u0435, \u043f\u043e \u0432\u0441\u0435\u043c \u043f\u0430\u0440\u0430\u043c\u0435\u0442\u0440\u0430\u043c \u0441\u043d\u043e\u0432\u0430 \u0432\u043e\u0437\u0432\u0440\u0430\u0449\u0430\u044e\u0441\u044c \u043a \u044d\u0442\u043e\u0439 \u043c\u043e\u0434\u0435\u043b\u0438. \u000a(\u041c\u043e\u0439 \u043a\u0443\u043f\u043b\u0435\u043d \u0431\u044b\u043b \u0437\u0430 3599 \u0440\u0443\u0431\u043b\u0435\u0439 11.09.2013 \u0433.)
p14
aV\u0445\u043e\u0440\u043e\u0448\u0438\u0439 \u0443\u0442\u044e\u0433 \u0437\u0430 \u0441\u0432\u043e\u0438 \u0434\u0435\u043d\u044c\u0433\u0438
